function lexicalName(func : Function)
{
    return func.name;
}

// Initialization 
const expDate = new Date();

// Members usage. 
expDate.getHours();
expDate.getTime();
expDate.toISOString();


console.log("A member of the class Date is Date." + lexicalName(expDate.getDate));

// Definition of class

class RomanDate
{
    year: number = 0;
    month: number = 0 ;
    day: number = 0;
}
// ^
// Note that the properties (variables) of the class do not have an scope prefix, instead TypeScript only uses the type. Excellent!






